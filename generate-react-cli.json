{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/app/component",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false
    },
    "class": {
      "path": "src/app/component",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "customTemplates":{
        "component": "grc/class/TemplateName.js"
      }
    },
    "funcHook": {
      "path": "src/app/component",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "customTemplates":{
        "component": "grc/function/TemplateName.js"
      }
    },
    "layoutFunc": {
      "path": "src/app/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": false,
      "customTemplates":{
        "component": "grc/class/TemplateName.js"
      }
    }
  }
}